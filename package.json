{
  "name": "@nestjs/fastify-passport",
  "version": "8.2.1",
  "description": "Nest - modern, fast, powerful node.js web framework (@passport)",
  "author": {
    "name": "Adward",
    "email": "adward0511@gmail.com",
    "url": "https://adward-nguyen-portfolio.vercel.app/"
  },
  "homepage": "https://www.passportjs.org/",
  "repository": {
    "type": "git",
    "url": "git://github.com/quocduvp/passport-fastify.git"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"lib/**/*.ts\"",
    "lint": "eslint 'lib/**/*.ts' --fix",
    "precommit": "lint-staged",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prerelease": "npm run build",
    "release": "release-it",
    "test": "jest"
  },
  "peerDependencies": {
    "@nestjs/common": "^6.0.0 || ^7.0.0 || ^8.0.0",
    "passport-fastify": "^0.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.1",
    "@commitlint/config-angular": "16.2.1",
    "@nestjs/common": "8.4.0",
    "@nestjs/core": "8.4.0",
    "@nestjs/jwt": "8.0.0",
    "@nestjs/platform-express": "8.4.0",
    "@nestjs/testing": "8.4.0",
    "@types/jest": "27.4.1",
    "@types/node": "17.0.17",
    "@types/passport-jwt": "3.0.6",
    "@types/passport-local": "1.0.34",
    "@typescript-eslint/eslint-plugin": "5.14.0",
    "@typescript-eslint/parser": "5.14.0",
    "eslint": "8.10.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.25.4",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "lint-staged": "12.3.5",
    "pactum": "3.1.4",
    "passport-fastify": "^0.0.1",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "prettier": "2.5.1",
    "reflect-metadata": "0.1.13",
    "release-it": "14.12.5",
    "rimraf": "3.0.2",
    "rxjs": "7.5.5",
    "ts-jest": "27.1.3",
    "typescript": "4.6.2"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -c .commitlintrc.json -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  }
}
